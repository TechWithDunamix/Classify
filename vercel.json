{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "classify/asgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/classify/asgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "mysite.settings",
      "PYTHONUNBUFFERED": "1"
    }
  }
  